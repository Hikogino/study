<%- include('partials/navigation') %>
<h1>Books</h1>
<% if (books.length) { %>
    <ul>
        <% books.forEach(function(book) { %>
            <li><%= book.title %> - <a href="/book/<%= book.id %>">View Details</a></li>
        <% }); %>
    </ul>
<% } else { %>
    <p>No books have been found.</p>
<% } %>



